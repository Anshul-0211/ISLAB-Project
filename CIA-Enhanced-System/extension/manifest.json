{
    "manifest_version": 3,
    "name": "Website Phishing Detector",
    "version": "1.0",
    "description": "Detects phishing websites and warns the user.",
    "permissions": ["scripting","downloads","storage", "activeTab", "notifications","tabs","<all_urls>", "webNavigation"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": ["http://localhost:5000/*"],
    "icons": {
      "48": "icon.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["warning.html"],
        "matches": ["<all_urls>"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon.png"
      }
    }
  }
  